{
  "name": "luke",
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "flowbite": "^2.5.2",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "jekyll": "bundle exec jekyll serve",
    "tailwind": "yarn dlx tailwindcss -i ./assets/css/main.css -o ./assets/css/styles.css --watch",
    "start": "TAILWIND_MODE=watch NODE_ENV=development yarn run jekyll & yarn run tailwind"
  }
}
